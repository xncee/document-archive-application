<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" prefWidth="1630.0" prefHeight="1640.0" style="-fx-background-color: #444444;">
    <StackPane layoutX="100.0" layoutY="100.0" prefWidth="1440.0" prefHeight="1440.0" style="-fx-background-color: white; -fx-border-color: #ced4da; -fx-border-width: 2px; -fx-border-radius: 8px;">
        <Pane style="-fx-background-color: rgba(0, 0, 0, 0.5);" prefWidth="1440.0" prefHeight="1440.0">
            <StackPane layoutX="464.0" layoutY="440.0" prefWidth="480.0" prefHeight="540.0" style="-fx-background-color: white; -fx-background-radius: 8px; -fx-border-color: #e5e7eb; -fx-border-width: 1px; -fx-effect: dropshadow(gaussian, rgba(0, 0, 0, 0.1), 10, 0, 0, 15);">
                <VBox spacing="24.0" padding="24.0" alignment="TOP_LEFT">
                    <HBox alignment="CENTER_LEFT" spacing="10.0">
                        <Text text="Filter Results" style="-fx-font-family: 'Inter'; -fx-font-size: 18px; -fx-font-weight: bold;" />
                        <ImageView fitWidth="15.0" fitHeight="20.0">
                            <Image url="@xIcon.png" />
                        </ImageView>
                    </HBox>
                    <VBox spacing="16.0">
                        <Text text="Status" style="-fx-font-family: 'Inter'; -fx-font-size: 14px; -fx-font-weight: bold;" />
                        <HBox spacing="10.0">
                            <Button text="Completed" style="-fx-background-color: transparent; -fx-border-color: #e5e7eb; -fx-border-radius: 6px; -fx-padding: 8px 16px;" />
                            <Button text="Pending" style="-fx-background-color: transparent; -fx-border-color: #e5e7eb; -fx-border-radius: 6px; -fx-padding: 8px 16px;" />
                            <Button text="Late" style="-fx-background-color: transparent; -fx-border-color: #e5e7eb; -fx-border-radius: 6px; -fx-padding: 8px 16px;" />
                        </HBox>
                    </VBox>
                    <VBox spacing="16.0">
                        <Text text="Department" style="-fx-font-family: 'Inter'; -fx-font-size: 14px; -fx-font-weight: bold;" />
                        <HBox>
                            <TextField style="-fx-background-color: #efefef; -fx-border-color: #d4d4d4; -fx-border-radius: 6px;" />
                            <ImageView fitWidth="16.0" fitHeight="16.0">
                                <Image url="@arrowDown.png" />
                            </ImageView>
                        </HBox>
                    </VBox>
                    <VBox spacing="16.0">
                        <Text text="Classification" style="-fx-font-family: 'Inter'; -fx-font-size: 14px; -fx-font-weight: bold;" />
                        <HBox>
                            <TextField style="-fx-background-color: #efefef; -fx-border-color: #d4d4d4; -fx-border-radius: 6px;" />
                            <ImageView fitWidth="16.0" fitHeight="16.0">
                                <Image url="@arrowDown.png" />
                            </ImageView>
                        </HBox>
                    </VBox>
                    <VBox spacing="16.0">
                        <Text text="Date Range" style="-fx-font-family: 'Inter'; -fx-font-size: 14px; -fx-font-weight: bold;" />
                        <HBox spacing="10.0">
                            <TextField promptText="Start Date" style="-fx-background-color: #ffffff; -fx-border-color: #d4d4d4; -fx-border-radius: 6px;" />
                            <TextField promptText="End Date" style="-fx-background-color: #ffffff; -fx-border-color: #d4d4d4; -fx-border-radius: 6px;" />
                        </HBox>
                    </VBox>
                    <HBox spacing="10.0">
                        <Button text="Reset" style="-fx-background-color: transparent; -fx-border-color: #e5e7eb; -fx-border-radius: 6px; -fx-padding: 8px 16px;" />
                        <Button text="Apply Filters" style="-fx-background-color: #171717; -fx-text-fill: white; -fx-border-radius: 6px; -fx-padding: 8px 16px;" />
                    </HBox>
                </VBox>
            </StackPane>
        </Pane>
    </StackPane>
</AnchorPane>
